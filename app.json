{
  "expo": {
    "name": "Behrend Immersive Experience",
    "slug": "VAR Labs App",
    "description": "Behrend Immersive App",
    "primaryColor": "#CB0C9F",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#CB0C9F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CB0C9F",
        "permissions": [
          "CAMERA",
          "ACCESS_FINE_LOCATION"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
